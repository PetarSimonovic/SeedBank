EN for \"+userId);_context.next=3;return src_api.getPlants(userId);case 3:loadedPlants=_context.sent;plantsData=loadedPlants.data.data;parsedPlants=[];today=setDate(Date()).stamp;for(plant=0;plant<plantsData.length;plant++){console.log(\"LOADED PLANT IS\");parsedPlant=plantsData[plant];parsedPlant.date=JSON.parse(parsedPlant.date);parsedPlant.growth=calculateGrowth(parsedPlant.date,today);console.log(parsedPlant);parsedPlants.push(Object.assign(new gameObjects_Plant_Plant(),parsedPlant));}return _context.abrupt(\"return\",parsedPlants);case 9:case\"end\":return _context.stop();}}},_callee);}));return _loadPlants.apply(this,arguments);}function calculateGrowth(plantDate,today){console.log(plantDate.stamp);console.log(today);var days=calculateDays(plantDate.stamp,today);return days;}//module.exports = parseGarden;\n// CONCATENATED MODULE: ./src/functions/LoadSeeds.js\nfunction loadSeeds(_x){return _loadSeeds.apply(this,arguments);}function _loadSeeds(){_loadSeeds=Object(asyncToGenerator[\"a\" /* default */])(/*#__PURE__*/regenerator_default.a.mark(function _callee(id){var loadedSeeds,seedsData,seedCollection,index,seed;return regenerator_default.a.wrap(function _callee$(_context){while(1){switch(_context.prev=_context.next){case 0:console.log(\"LOADING Seeds FOR \"+id);_context.next=3;return src_api.getSeeds(id);case 3:loadedSeeds=_context.sent;seedsData=loadedSeeds.data.data;console.log(seedsData);seedCollection=[];for(index=0;index<seedsData.length;index++){seed=seedsData[index];seedCollection.push({type:seed.type,quantity:seed.quantity});}return _context.abrupt(\"return\",seedCollection);case 9:case\"end\":return _context.stop();}}},_callee);}));return _loadSeeds.apply(this,arguments);}\n// CONCATENATED MODULE: ./src/functions/MakeFriendRequest.js\nfunction makeFriendRequest(_x,_x2,_x3){return _makeFriendRequest.apply(this,arguments);}function _makeFriendRequest(){_makeFriendRequest=Object(asyncToGenerator[\"a\" /* default */])(/*#__PURE__*/regenerator_default.a.ma